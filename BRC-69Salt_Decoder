import hashlib
import base64

salt = "Salt6969420741!"

# Hash values
hash_value1 = "paste_hash_value_1_here"
hash_value2 = "paste_hash_value_2_here"

# Reverse the hashing process to retrieve the base64 strings
decoded_data1 = base64.b64decode(hash_value1[len(salt):]).decode('utf-8')
decoded_data2 = base64.b64decode(hash_value2[len(salt):]).decode('utf-8')

# Write the decoded base64 strings to a text file
with open('decoded_strings.txt', 'w') as file:
    file.write(f'BRC-69: {decoded_data1}\n')
    file.write(f'BRC-69_MD: {decoded_data2}\n')

# Confirmation message
print("Decoded strings have been written to the file 'decoded_strings.txt'.")
